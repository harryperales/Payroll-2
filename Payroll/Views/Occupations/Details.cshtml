@model Payroll.Models.Occupation

<div class="row" align="left">
    <div class="col-md-12">
        <form>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" disabled="disabled" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="IsRegulated" disabled="disabled" /> @Html.DisplayNameFor(model => model.IsRegulated)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <label asp-for="CouncilName" class="control-label"></label>
                <input asp-for="CouncilName" class="form-control" disabled="disabled" />
                <span asp-validation-for="CouncilName" class="text-danger"></span>
            </div>
            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="CreatedAt" />
            <input type="hidden" asp-for="CreatedBy" />
            <div class="form-group" align="right">
                <a class="btn btn-default" asp-action="Index">@Resource.Back</a>
            </div>
        </form>
    </div>
</div>


